<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.xiao.springbootinit.mapper.PostMapper">

    <select id="listPostWithDelete" resultType="cn.xiao.springbootinit.model.entity.Post">
        select *
        from t_post
        where update_time >= #{minUpdateTime}
    </select>

</mapper>
